public class IscrtajTabelu {
	private ArrayList<Tabela> listaTabela=new ArrayList<>();
	private Tabela tabela=new Tabela();
	private String ime="";
	
	public IscrtajTabelu(ArrayList<Tabela> tabela,String ime) {
		this.listaTabela=tabela;
		this.ime=ime;
	}
	
	public JPanel iscrtavanje() {
		JPanel p=new JPanel();
		
		for(Tabela t:listaTabela) {
			if(t.getTitle().equals(ime)) {}
			tabela=t;
		}
		
		
		
		//JLabel l=new JLabel("ASKDJJJSKJSJDKF");
		//p.add(l);
		return p;
		
		
	}
}
